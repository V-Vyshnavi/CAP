public static void miniMaxSum(List<Integer> arr) {
    // Write your code here
    long min=0,max=0;
    Collections.sort(arr);
    for(int i=0;i<arr.size()-1;i++){
        max += arr.get(i);
    }
    for(int j=1;j<arr.size();j++){
        min += arr.get(j);
    }
    System.out.printf("%d %d", max,min);

    

    }

}
