class Solution {
    public int firstUniqChar(String s) {
        int len = s.length();
        String str;
        for(int i=0;i<len;i++){
            if(i==len-1){
                str = s.substring(0,i);
            }
            else{
                str = s.substring(0,i)+s.substring(i+1);
            }
            if(!str.contains(s.charAt(i)+ ""))
            return i;
        }
        return -1;
    }
}
