public static int lonelyinteger(List<Integer> a) {
    // Write your code here
    List<Integer> list = new ArrayList<>();
    int n = a.size();
    for(int i=0;i<n;i++){
        if(!list.contains(a.get(i))){
            list.add(a.get(i));
        }
        else{
            list.remove(a.get(i));
        }
    }
    return (list.get(0));
    }

}
